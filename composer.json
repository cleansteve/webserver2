{
    "name": "toolbox/webserver",
    "type": "project",
    "description": "Try to find out what's possible",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "bensampo/laravel-enum": "^v3.4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.3",
        "phpstan/phpstan": "^0.12.40"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Src\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
        ],
        "post-root-package-install": [
        ],
        "post-create-project-cmd": [
        ],
        "post-install-cmd": [
        ]
    }
}
